{
  "version": 2,
  "framework": "vite",
  "buildCommand": "npm run build:client",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://your-backend-url.com"
  }
}


